{
  "extends": ["next/core-web-vitals", "eslint:recommended", "next", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "quotes": ["error", "double"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "trailingComma": "all",
        "tabWidth": 4,
        "bracketSpacing": false
      }
    ]
  }
}
